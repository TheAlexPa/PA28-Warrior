		<object-name>RightFlap</object-name>
		<object-name>GearSub</object-name>
		<object-name>Spinner</object-name>
		<object-name>Propeller</object-name>
		<object-name>VHFAntenna.1</object-name>
		<object-name>VHFAntenna.2</object-name>
		<!-- Fuselage -->
		<property-base>sim/model/livery</property-base>
		<texture-prop>texture</texture-prop>
		<texture>PA28-WarriorII.png</texture>
	</animation>

	<animation>
		<type>material</type>
		<object-name>prop disc rear</object-name>
		<property-base>sim/model/livery</property-base>
		<texture-prop>reardisc</texture-prop>
		<texture>Liveries/propblur.png</texture>
	</animation>

	<animation>
		<type>material</type>
		<object-name>prop disc front</object-name>
		<property-base>sim/model/livery</property-base>
		<texture-prop>frontdisc</texture-prop>
		<texture>Liveries/propblur.png</texture>
	</animation>

	<!-- Nasal code -->
	<nasal>
		<load>
		var livery_update = aircraft.livery_update.new("Aircraft/PA28-Warrior/Models/Liveries");
		</load>
		<unload>
		livery_update.stop();
		</unload>
	</nasal>

	<effect>
		<condition>
		<property>/sim/rendering/rembrandt/enabled</property>
		</condition>
		<inherits-from>Effects/model-transparent</inherits-from>
		<object-name>prop disc front</object-name>
		<object-name>prop disc rear</object-name>
	</effect>

